    "typescript": "^5.3.3"
  },
  "resolutions": {
    "cookie": "^0.7.1",
    "@auth/core/cookie": "^0.7.1"
  },
  "_comment": "Security overrides: Force newer versions of vulnerable dependencies until full migration",
  "overrides": {
    "next-auth": {
      "cookie": "^0.7.1"
    },
    "prismjs": "1.30.0"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --config .eslintrc.js --fix",
      "prettier --write"
    ],
    "src/**/*.{js,jsx,json,css,scss}": [
      "prettier --write"
    ]
  }
}

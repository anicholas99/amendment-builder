    "mammoth": "^1.9.1",
    "markitdown": "^0.0.4",
    "mermaid": "^11.5.0",
    "next": "^15.2.4",
    "openai": "^4.24.1",
    "pdf-parse": "^1.1.1",
    "rate-limit-redis": "^4.2.1",
    "re-resizable": "^6.11.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^4.12.0",
    "react-markdown": "^9.1.0",
    "react-window": "^1.8.11",
    "reactflow": "^11.10.1",
    "remark-breaks": "^4.0.0",

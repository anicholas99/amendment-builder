{
  "name": "patent-drafter-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run azurite\" \"next dev --turbo\"",
    "dev:staging": "cross-env NODE_ENV=production NEXT_PUBLIC_APP_ENV=qa npm run dev -- -p 3001",
    "dev:prod": "cross-env NODE_ENV=production NEXT_PUBLIC_APP_ENV=production npm run dev -- -p 3002",
    "env:dev": "node scripts/set-env.js development",
    "env:staging": "node scripts/set-env.js staging",
    "env:prod": "node scripts/set-env.js production",
    "azurite": "azurite --silent --location azurite-data --debug azurite-debug.log",
    "validate:security": "node scripts/validate-api-security.js",
    "track:migration": "node scripts/track-middleware-migration.js",
    "migration:progress": "node scripts/migration-progress.js",
    "migration:analyze": "node scripts/analyze-remaining-routes.js",
    "migration:detect-unguarded": "node scripts/detect-unguarded-mutations.js",
    "build": "npm run validate:security && next build",
    "build:dev": "npm run validate:security && NODE_ENV=development next build",
    "build:no-trace": "set NEXT_TELEMETRY_DISABLED=1 && set NEXT_DISABLE_SOURCEMAPS=1 && next build",
    "build-skip-types": "cp tsconfig.json tsconfig.json.bak && echo '{\"extends\": \"./tsconfig.json\", \"compilerOptions\": {\"noEmit\": false, \"skipLibCheck\": true}}' > tsconfig.skip.json && NEXT_TYPESCRIPT_CONFIG_PATH=tsconfig.skip.json next build && mv tsconfig.json.bak tsconfig.json",
    "start": "next start",
    "start-polling": "node scripts/start-citation-polling.js",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:migrate-dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:update-patterns": "node scripts/update-db-patterns.js",
    "db:seed": "ts-node -r tsconfig-paths/register src/lib/db/seed/seed.ts",
    "db:seed:force": "ts-node -r tsconfig-paths/register src/lib/db/seed/seed.ts --force",
    "db:seed:quiet": "ts-node -r tsconfig-paths/register src/lib/db/seed/seed.ts --quiet",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:delete-projects": "ts-node -r tsconfig-paths/register scripts/delete-all-projects.ts",
    "migrate:search-data": "ts-node -r tsconfig-paths/register scripts/migrate-search-data.ts",
    "security-audit": "node scripts/security-audit.js",
    "find-unused": "ts-prune",
    "setup-tenant": "ts-node scripts/setup-development-tenant.ts",
    "fix:user-tenant": "tsx scripts/fix-user-tenant-access.ts",
    "debug:user-tenant": "tsx scripts/debug-user-tenant.ts",
    "debug:view-gpt": "node scripts/view-latest-gpt-response.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "lint:fix": "next lint --fix",
    "fix:all": "npm run format && npm run lint:fix",
    "find:any": "grep -r \"any\" src/ --include=\"*.ts\" --include=\"*.tsx\"",
    "find:console": "grep -r \"console\\.\" src/ --include=\"*.ts\" --include=\"*.tsx\"",
    "find:unused": "ts-prune",
    "cleanup:analyze": "node find-unused-files.js",
    "cleanup:safe": "node scripts/safe-cleanup.js",
    "cleanup:check-imports": "npx madge --circular src",
    "cleanup:ts-check": "tsc --noEmit",
    "cleanup": "node scripts/cleanup-code.js",
    "cleanup:unused-deps": "npx depcheck",
    "cleanup:bundle-analysis": "cross-env ANALYZE=true next build",
    "cleanup:security": "npm audit --audit-level=moderate",
    "fix:logger-imports": "node scripts/fix-logger-imports.js",
    "fix:quick": "npm run fix:logger-imports && npm run lint:fix",
    "test": "jest",
    "test:node": "jest --config jest.config.node.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:redis": "ts-node scripts/test-redis-connection.ts",
    "check:production": "node scripts/production-readiness-check.js",
    "audit:chakra": "node scripts/audit-chakra-imports.js",
    "type-check": "tsc --noEmit",
    "audit:csrf": "ts-node scripts/audit/audit-csrf.ts",
    "audit:env": "ts-node scripts/audit/audit-env-vars.ts",
    "audit:console": "ts-node scripts/audit/audit-console.ts",
    "audit:all": "npm run audit:csrf && npm run audit:env && npm run audit:console",
    "analyze:routes": "ts-node src/lib/dev/route-analyzer.ts",
    "analyze:route": "ts-node src/lib/dev/route-analyzer.ts",
    "setup:precommit": "node scripts/setup-precommit-hooks.js",
    "pre-commit": "npm run type-check",
    "prepare": "husky",
    "test:security": "npm audit --audit-level=high",
    "lint:security": "eslint . --ext .ts,.tsx --config .eslintrc.security.js",
    "check:env": "node scripts/check-env-security.js",
    "security:scan": "npm run test:security && npm run lint:security && npm run check:env",
    "audit:api": "tsx scripts/analyze-api-security.ts",
    "audit:full": "npm run security:scan && npm run audit:api",
    "verify:dark-mode": "tsx scripts/verify-dark-mode.ts",
    "precommit": "npm run lint && npm run type-check && npm run test:security",
    "prepush": "npm run audit:full",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.7.0",
    "@azure/storage-blob": "^12.27.0",
    "@azure/storage-queue": "^12.26.0",
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/is-prop-valid": "^1.2.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.17",
    "@mdx-js/react": "^3.0.1",
    "@prisma/client": "^6.6.0",
    "@tanstack/react-query": "^5.74.4",
    "@tiptap/extension-character-count": "^2.14.0",
    "@tiptap/extension-font-family": "^2.23.0",
    "@tiptap/extension-heading": "^2.11.7",
    "@tiptap/extension-subscript": "^2.14.0",
    "@tiptap/extension-superscript": "^2.14.0",
    "@tiptap/extension-text-align": "^2.11.7",
    "@tiptap/extension-text-style": "^2.12.0",
    "@tiptap/extension-underline": "^2.11.7",
    "@tiptap/react": "^2.11.7",
    "@tiptap/starter-kit": "^2.11.7",
    "@types/dompurify": "^3.0.5",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^4.28.10",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "diff": "^7.0.0",
    "docx": "^9.2.0",
    "dompurify": "^3.2.6",
    "draft-js": "^0.11.7",
    "express-rate-limit": "^7.5.0",
    "file-saver": "^2.0.5",
    "file-type": "^19.1.0",
    "formidable": "^3.5.3",
    "framer-motion": "^10.18.0",
    "ioredis": "^5.6.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "lru-cache": "^10.2.0",
    "mammoth": "^1.9.1",
    "mermaid": "^11.5.0",
    "next": "^15.2.4",
    "openai": "^4.24.1",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.5.0",
    "re-resizable": "^6.11.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.12.0",
    "react-markdown": "^9.1.0",
    "react-window": "^1.8.11",
    "reactflow": "^11.10.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "slate": "^0.103.0",
    "slate-react": "^0.107.1",
    "stemmer": "^2.0.1",
    "ts-morph": "^25.0.1",
    "use-debounce": "^10.0.5",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.3",
    "@tanstack/react-query-devtools": "^5.74.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/diff": "^7.0.2",
    "@types/draft-js": "^0.11.18",
    "@types/file-saver": "^2.0.7",
    "@types/formidable": "^3.4.5",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.11.19",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/react-window": "^1.8.8",
    "@types/slate": "^0.47.16",
    "@types/slate-react": "^0.50.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.2.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-local": "^4.2.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "glob": "^11.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-mock-extended": "^4.0.0-beta1",
    "lint-staged": "^16.1.0",
    "node-mocks-http": "^1.16.2",
    "prettier": "^3.5.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "cookie": "^0.7.1",
    "@auth/core/cookie": "^0.7.1"
  },
  "_comment": "Security overrides: Force newer versions of vulnerable dependencies until full migration",
  "overrides": {
    "next-auth": {
      "cookie": "^0.7.1"
    },
    "prismjs": "1.30.0"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{js,jsx,json,css,scss}": [
      "prettier --write"
    ]
  }
}
